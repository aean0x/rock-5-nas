# SOPS Secrets Template
# 1. Copy to secrets.yaml.work
# 2. Fill in your values
# 3. Run ./encrypt

# Generate with: mkpasswd -m SHA-512
user_hashedPassword: "$6$..."

# Tailscale auth key (from https://login.tailscale.com/admin/settings/keys)
tailscale_authKey: "tskey-auth-..."

# WiFi PSK (only needed if enableWifi = true in settings.nix)
# wifi_psk: "your_wifi_password"

# OpenClaw gateway token
openclaw_gateway_token: "tok_..."
openclaw_gateway_password: "your_gateway_password"

# OpenRouter API key (for OpenClaw)
openrouter_api_key: "sk-or-..."

# Anthropic API key (for OpenClaw direct access)
anthropic_api_key: "sk-ant-..."

# Brave Search API key (for OpenClaw web search)
brave_search_api_key: "BSA..."

# Telegram bot token (for OpenClaw channels)
telegram_bot_token: "123456:ABC..."

# Composio self-hosted secrets
composio_encryption_key: "your_encryption_key"
composio_jwt_secret: "your_jwt_secret"

# Google Workspace MCP (OAuth client credentials for OpenClaw)
google_workspace_client_id: "261437751113-....apps.googleusercontent.com"
google_workspace_client_secret: "GOCSPX-..."

# OneDrive rclone config (full rclone config file content)
onedrive_rclone_config: |
  [onedrive]
  type = onedrive
  token = {"access_token":"...","token_type":"Bearer","refresh_token":"...","expiry":"..."}
  drive_id = ...
  drive_type = personal

# Cloudflared tunnel credentials JSON (from: cloudflared tunnel create rocknas)
# Paste the full JSON content from ~/.cloudflared/<uuid>.json
cloudflared_tunnel_credentials: |
  {"AccountTag":"...","TunnelID":"...","TunnelSecret":"..."}
